" set tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab
" syntax on
" set autoindent
" set number

" Automatic end brackets
" inoremap " ""<left>
" inoremap ' ''<left>
" inoremap ( ()<left>
" inoremap [ []<left>
" inoremap { {}<left>
" inoremap {<CR> {<CR>}<ESC>O
" inoremap {;<CR> {<CR>};<ESC>O

